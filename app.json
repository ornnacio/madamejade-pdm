{
	"expo": {
		"name": "madamejade",
		"slug": "madamejade",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/splash2.png",
			"resizeMode": "contain",
			"backgroundColor": "#aef490"
		},
		"updates": {
